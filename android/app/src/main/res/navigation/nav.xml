<?xml version="1.0" encoding="utf-8"?>


<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav"
    app:startDestination="@id/Personajes">

    <fragment
        android:id="@+id/Personajes"
        android:label="Personajes"
        android:name="com.example.wikirick.Personajes.PersonajesFragment"
        tools:layout="@layout/personajes_fragment">
        <argument
            android:name="text_id"
            android:defaultValue="@string/personajes"
            app:argType="reference" />
    </fragment>
    <fragment
        android:label="Perfil"
        android:id="@+id/Perfil"
        android:name="com.example.wikirick.Perfil.PerfilFragment"
        tools:layout="@layout/perfil_fragment">
        <argument
            android:name="text_id"
            android:defaultValue="@string/perfil"
            app:argType="reference" />
    </fragment>
    <fragment
        android:label="Episodios"
        android:id="@+id/Episodios"
        android:name="com.example.wikirick.Episodios.EpisodiosFragment"
        tools:layout="@layout/episodios_fragment">
        <argument
            android:name="text_id"
            android:defaultValue="@string/episodios"
            app:argType="reference" />
    </fragment>


</navigation>